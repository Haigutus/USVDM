ation>					<xsd:documentation>						<CodeDescription>							<Title>Culprit clearing confirmation</Title>							<Definition>The confirmation of the quantities that caused a network imbalance.</Definition>						</CodeDescription>					</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ALT">				<xsd:annotation>					<xsd:documentation>						<CodeDescription>							<Title>Emergency clearing confirmation</Title>							<Definition>The confirmation of the quantities provided for resolving an emergency situation to ensure network balance.</Definition>						</CodeDescription>					</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ALU">				<xsd:annotation>					<xsd:documentation>						<CodeDescription>							<Title>Balancing Agreement</Title>							<Definition>The document identifying the balancing agreement that has been contracted between two parties.</Definition>						</CodeDescription>					</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ALV">				<xsd:annotation>					<xsd:documentation>						<CodeDescription>							<Title>Balancing Anomaly</Title>							<Definition>The document identifying anomalies that have occured in the matching of two balancing agreements.</Definition>						</CodeDescription>					</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ALW">				<xsd:annotation>					<xsd:documentation>						<CodeDescription>							<Title>Balancing Confirmation</Title>							<Definition>The document confirming that the balancing agreement between two parties has been confirmed.</Definition>						</CodeDescription>					</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>			<xsd:enumeration value="ALX">				<xsd:annotation>					<xsd:documentation>						<CodeDescription>							<Title>Imbalance limits document</Title>							<Definition>The document providing imbalance